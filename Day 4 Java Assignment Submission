import java.util.Scanner;  
public class Main 
{  
public static void main(String args[])  
{  
int x, y, sum;  
Scanner sc = new Scanner(System.in);  
 x = sc.nextInt();  
  y = sc.nextInt();  
sum = sum(x, y);  
System.out.println(sum);  
}  
public static int sum(int a, int b)  
{  
int sum = a + b;  
return sum;  
}  
}  
